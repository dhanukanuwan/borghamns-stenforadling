{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "borghamns-general/slideshow",
	"version": "0.1.0",
	"title": "Slide Show",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"images": {
			"type": "array",
			"default": []
		},
		"slides": {
			"type": "integer",
			"default": 1
		},
		"speed": {
			"type": "integer",
			"default": 500
		},
		"navigation": {
			"type": "boolean",
			"default": true
		},
		"pagination": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "slideshow",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}